{
  "name": "stencil-app",
  "version": "1.0.0",
  "description": "Stencil boilerplate",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build-storybook": "build-storybook -s www",
    "build": "stencil build --stats --prerender",
    "deploy": "gh-pages -d www",
    "dev": "stencil build --dev --watch --serve",
    "lint": "sd concurrent \"npm run lint:ts\" \"npm run lint:styles\"",
    "lint:styles": "stylelint \"src/**/*.css\"",
    "lint:ts": "tslint -p tsconfig.json -c tslint.json",
    "generate-addon-jest-testresults": "jest --config=tests/addon-jest.config.json --json --outputFile=stories/addon-jest.testresults.json",
    "serve": "stencil-dev-server",
    "serve:prod": "npm run build && serve ./www",
    "start": "sd concurrent \"npm run dev\" \"npm run storybook\"",
    "storybook": "start-storybook -p 9006 -s www",
    "test": "jest --no-cache",
    "test:watch": "jest --watch --no-cache"
  },
  "author": "Jag Reehal",
  "license": "ISC",
  "dependencies": {
    "@stencil/core": "0.11.2"
  },
  "devDependencies": {
    "@stencil/postcss": "0.1.0",
    "@stencil/utils": "^0.0.4",
    "@storybook/addon-a11y": "4.0.0-alpha.11",
    "@storybook/addon-actions": "4.0.0-alpha.11",
    "@storybook/addon-backgrounds": "4.0.0-alpha.11",
    "@storybook/addon-centered": "4.0.0-alpha.11",
    "@storybook/addon-events": "4.0.0-alpha.9",
    "@storybook/addon-jest": "4.0.0-alpha.11",
    "@storybook/addon-knobs": "4.0.0-alpha.11",
    "@storybook/addon-links": "4.0.0-alpha.11",
    "@storybook/addon-notes": "4.0.0-alpha.11",
    "@storybook/addon-options": "4.0.0-alpha.11",
    "@storybook/addon-storyshots": "4.0.0-alpha.11",
    "@storybook/addon-storysource": "4.0.0-alpha.9",
    "@storybook/addon-viewport": "4.0.0-alpha.11",
    "@storybook/addons": "4.0.0-alpha.11",
    "@storybook/core": "4.0.0-alpha.11",
    "@storybook/core-events": "4.0.0-alpha.11",
    "@storybook/html": "4.0.0-alpha.11",
    "@types/jest": "^23.3.1",
    "autoprefixer": "^9.1.0",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "eventemitter3": "^3.1.0",
    "format-json": "^1.0.3",
    "gh-pages": "1.2.0",
    "global": "^4.3.2",
    "husky": "^0.15.0-rc.9",
    "jest": "^23.4.2",
    "postcss-custom-media": "^6.0.0",
    "postcss-import-url": "^3.0.4",
    "postcss-nested": "^3.0.0",
    "postcss-reporter": "^5.0.0",
    "prettier": "^1.14.0",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "5.9.2",
    "tslint-config-prettier": "^1.14.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm test"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
