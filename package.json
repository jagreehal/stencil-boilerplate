{
  "name": "stencil-app",
  "version": "1.0.0",
  "description": "Stencil boilerplate",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build-storybook": "build-storybook -s www",
    "build": "NODE_ENV=production stencil build --stats",
    "publish": "gh-pages -d www",
    "dev": "stencil build --watch --serve --next",
    "lint": "run-p \"lint.ts\" \"lint.styles\"",
    "lint.styles": "stylelint \"src/**/*.css\"",
    "lint.ts": "eslint src/**/*.tsx",
    "serve": "serve www -p 5000",
    "start": "run-p dev storybook",
    "storybook": "start-storybook -p 9006 -s www",
    "stylelint:fix": "stylelint --fix \"src/**/*.css\"",
    "test": "stencil test --spec --e2e --headless",
    "test.ci": "stencil test --spec --e2e --ci",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "test.unit": "stencil test --spec",
    "test.e2e": "stencil test --e2e --headless",
    "test.screenshot": "stencil test --e2e --screenshot"
  },
  "author": "Jag Reehal",
  "license": "ISC",
  "dependencies": {
    "@stencil/core": "1.8.7"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "@stencil/eslint-plugin": "^0.2.1",
    "@stencil/postcss": "^1.0.1",
    "@storybook/addon-a11y": "5.3.12",
    "@storybook/addon-docs": "5.3.12",
    "@storybook/addon-knobs": "5.3.12",
    "@storybook/addon-viewport": "5.3.12",
    "@storybook/web-components": "5.3.12",
    "@types/autoprefixer": "^9.6.1",
    "@types/babel-core": "^6.25.6",
    "@types/cssnano": "^4.0.0",
    "@types/gh-pages": "^2.0.1",
    "@types/jest": "24.0.25",
    "@types/node": "^13.7.0",
    "@types/prettier": "^1.19.0",
    "@types/puppeteer": "1.20.3",
    "@types/stylelint": "^9.10.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "autoprefixer": "^9.7.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-react": "^7.18.3",
    "gh-pages": "^2.2.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "lit-html": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.0",
    "puppeteer": "1.20.0",
    "stylelint": "^13.1.0",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "^1.2.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "preset": "@stencil/core/testing"
  }
}
