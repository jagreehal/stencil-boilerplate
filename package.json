{
  "name": "stencil-app",
  "version": "1.0.0",
  "description": "Stencil boilerplate",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.ms",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build-storybook": "build-storybook -s dist/stencil-app",
    "build": "NODE_ENV=production stencil build --stats",
    "dev": "stencil build --dev --watch --serve",
    "lint": "eslint src/**/*.tsx",
    "serve": "serve www -p 5000",
    "start": "run-p dev storybook",
    "storybook": "start-storybook -p 9006 -s www/build",
    "stylelint:fix": "stylelint --fix \"src/**/*.css\"",
    "test": "stencil test --spec --e2e",
    "test.ci": "npm run test",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "author": "Jag Reehal",
  "license": "ISC",
  "devDependencies": {
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/core": "^2.3.0",
    "@stencil/postcss": "^2.0.0",
    "@storybook/addon-a11y": "6.1.11",
    "@storybook/addon-docs": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-knobs": "6.1.11",
    "@storybook/addon-viewport": "6.1.11",
    "@storybook/web-components": "6.1.11",
    "@types/autoprefixer": "^9.7.2",
    "@types/babel-core": "^6.25.6",
    "@types/jest": "26.0.19",
    "@types/node": "^14.14.16",
    "@types/prettier": "^2.1.6",
    "@types/puppeteer": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react": "^7.22.0",
    "glob": "^7.1.6",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "lit-html": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "puppeteer": "^5.4.1",
    "storybook-addon-web-components-knobs": "^0.3.20",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "@stencil/core/testing"
  }
}
