{
  "name": "stencil-app",
  "version": "1.0.0",
  "description": "Stencil boilerplate",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build-storybook": "build-storybook -s www",
    "build": "NODE_ENV=production stencil build --stats",
    "publish": "gh-pages -d www",
    "dev": "stencil build --watch --serve",
    "lint": "eslint src/**/*.tsx",
    "serve": "serve www -p 5000",
    "start": "run-p dev storybook",
    "storybook": "start-storybook -p 9006 -s www",
    "stylelint:fix": "stylelint --fix \"src/**/*.css\"",
    "test": "stencil test --spec --e2e",
    "test.ci": "npm run test",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "author": "Jag Reehal",
  "license": "ISC",
  "dependencies": {
    "@stencil/core": "1.12.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/postcss": "^1.0.1",
    "@storybook/addon-a11y": "5.3.18",
    "@storybook/addon-docs": "5.3.18",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/addon-viewport": "5.3.18",
    "@storybook/web-components": "5.3.18",
    "@types/autoprefixer": "^9.7.2",
    "@types/babel-core": "^6.25.6",
    "@types/cssnano": "^4.0.0",
    "@types/gh-pages": "^2.2.0",
    "@types/jest": "25.1.5",
    "@types/node": "^13.11.0",
    "@types/prettier": "^2.0.0",
    "@types/puppeteer": "2.0.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "autoprefixer": "^9.7.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-react": "^7.19.0",
    "gh-pages": "^2.2.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "lit-html": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "puppeteer": "3.0.0",
    "tailwindcss": "^1.2.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "@stencil/core/testing"
  }
}
