{
  "name": "stencil-app",
  "version": "1.0.0",
  "description": "Stencil boilerplate",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build-storybook": "build-storybook -s www",
    "build": "stencil build --stats --prerender",
    "deploy": "gh-pages -d www",
    "dev": "stencil build --dev --watch --serve",
    "lint": "sd concurrent \"npm run lint.ts\" \"npm run lint.styles\"",
    "lint.styles": "stylelint \"src/**/*.css\"",
    "lint.ts": "tslint -p tsconfig.json -c tslint.json",
    "generate-addon-jest-testresults": "jest --config=tests/addon-jest.config.json --json --outputFile=stories/addon-jest.testresults.json",
    "serve": "stencil-dev-server",
    "serve.prod": "npm run build && serve ./www",
    "start": "sd concurrent \"npm run dev\" \"npm run storybook\"",
    "storybook": "start-storybook -p 9006 -s www",
    "stylelint:fix": "stylelint --fix \"src/**/*.css\"",
    "test": "stencil test --spec --e2e --headless",
    "test.ci": "stencil test --spec --e2e --ci",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "test.unit": "stencil test --spec",
    "test.e2e": "stencil test --e2e --headless",
    "test.screenshot": "stencil test --e2e --screenshot"
  },
  "author": "Jag Reehal",
  "license": "ISC",
  "dependencies": {
    "@stencil/core": "v0.14.0-5"
  },
  "devDependencies": {
    "@stencil/postcss": "0.1.0",
    "@stencil/utils": "^0.0.5",
    "@storybook/addon-a11y": "4.0.0-alpha.23",
    "@storybook/addon-actions": "4.0.0",
    "@storybook/addon-backgrounds": "4.0.0",
    "@storybook/addon-centered": "4.0.0",
    "@storybook/addon-events": "4.0.0-alpha.23",
    "@storybook/addon-jest": "4.0.0-alpha.23",
    "@storybook/addon-knobs": "4.0.0",
    "@storybook/addon-links": "4.0.0",
    "@storybook/addon-notes": "4.0.0",
    "@storybook/addon-options": "4.0.0",
    "@storybook/addon-storyshots": "4.0.0",
    "@storybook/addon-storysource": "4.0.0-alpha.23",
    "@storybook/addon-viewport": "4.0.0-alpha.23",
    "@storybook/addons": "4.0.0",
    "@storybook/core": "4.0.0-alpha.23",
    "@storybook/core-events": "4.0.0-alpha.23",
    "@storybook/html": "4.0.0-alpha.23",
    "@types/jest": "^23.3.3",
    "@types/puppeteer": "1.9.0",
    "autoprefixer": "^9.1.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-runtime": "^6.26.0",
    "eventemitter3": "^3.1.0",
    "format-json": "^1.0.3",
    "gh-pages": "2.0.1",
    "global": "^4.3.2",
    "husky": "^1.1.1",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "pixelmatch": "4.0.2",
    "postcss-custom-media": "^7.0.4",
    "postcss-import-url": "^3.0.4",
    "postcss-nested": "^4.1.0",
    "postcss-reporter": "^6.0.0",
    "prettier": "^1.14.3",
    "puppeteer": "1.8.0",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "5.11.0",
    "tslint-config-prettier": "^1.15.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm test"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
