{
  "name": "stencil-app",
  "version": "1.0.0",
  "description": "Stencil boilerplate",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build-storybook": "build-storybook -s www",
    "build": "NODE_ENV=production stencil build --stats",
    "dev": "stencil build --dev --watch --serve",
    "lint": "eslint src/**/*.tsx",
    "serve": "serve www -p 5000",
    "start": "run-p dev storybook",
    "storybook": "start-storybook -p 9006 -s www",
    "stylelint:fix": "stylelint --fix \"src/**/*.css\"",
    "test": "stencil test --spec --e2e",
    "test.ci": "npm run test",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "author": "Jag Reehal",
  "license": "ISC",
  "dependencies": {
    "@stencil/core": "^1.14.0"
  },
  "devDependencies": {
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/postcss": "^1.0.1",
    "@storybook/addon-a11y": "5.3.19",
    "@storybook/addon-docs": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/addon-viewport": "5.3.19",
    "@storybook/web-components": "5.3.19",
    "@types/autoprefixer": "^9.7.2",
    "@types/babel-core": "^6.25.6",
    "@types/jest": "25.2.3",
    "@types/node": "^14.0.6",
    "@types/prettier": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.0",
    "glob": "^7.1.6",
    "jest": "26.0.1",
    "jest-cli": "26.0.1",
    "lit-html": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.9.3"
  },
  "jest": {
    "preset": "@stencil/core/testing"
  }
}
